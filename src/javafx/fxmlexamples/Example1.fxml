<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.CheckBox?>

<HBox xmlns:fx="http://javafx.com/fxml"
	fx:controller="javafx.fxmlexamples.Example1Controller"
	>
	<TextField fx:id="textOutput"
		text="Type something here!"
		onTextChange="#handleTextChange"
	/>
	<CheckBox selected="true" onSelectedChange="#checkBoxSelected"/>
	<Button text="Click me!" onAction="#handleClickMeAction"/>
	<!--
	<Button fx:id="clickMeButton" text="Click me!"/>
	  -->
</HBox>
