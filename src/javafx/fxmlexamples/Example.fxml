<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.CheckBox?>

<HBox xmlns:fx="http://javafx.com/fxml" fx:controller="javafx.fxmlexamples.ExampleController">
  <TextField fx:id="textInput" text="Type something here!"
  	onTextChange="#handleTextChange"
  	/>
  <CheckBox fx:id="autoUpcase"/>
  <Button text="Upcase" onAction="#handleUpcaseAction"/>
</HBox>
